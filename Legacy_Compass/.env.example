# Legacy Compass Environment Variables
# Copy this file to .env and fill in your values

# Mapping (Required)
MAPBOX_TOKEN=pk.your_mapbox_token_here

# AI/LLM (Optional - AI features won't work without these)
GHOST_API_URL=https://your-ghost-instance.com
GROQ_API_KEY=gsk_your_groq_key_here
OPENAI_API_KEY=sk-your_openai_key_here

# Data Enrichment (Optional)
BROWSER_CLOUD_KEY=your_browser_cloud_key
SPOKEO_API_KEY=your_spokeo_key
BEENVERIFIED_KEY=your_beenverified_key

# Communication (Optional)
TWILIO_ACCOUNT_SID=your_twilio_sid
TWILIO_AUTH_TOKEN=your_twilio_token
TWILIO_PHONE_NUMBER=+1234567890

# Database (Optional - for sync features)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key

# Analytics (Optional)
VERCEL_ANALYTICS_ID=your_analytics_id
SENTRY_DSN=your_sentry_dsn

# Multi-tenant (Optional)
DEFAULT_TENANT=default
MAX_PROPERTIES_PER_TENANT=5000
ENRICHMENT_CREDITS_PER_MONTH=1000

# Development
NODE_ENV=development
DEBUG=false